{
  "name": "pair-programming-practice",
  "version": "1.0.0",
  "description": "Practice problems for Node.js, TypeScript, async/await, promises",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "run": "tsx",
    "new": "tsx src/scripts/new-problem.ts",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint . && eslint .",
    "lint:fix": "biome lint --write . && eslint --fix .",
    "check": "biome check --write ."
  },
  "keywords": [
    "typescript",
    "nodejs",
    "practice",
    "interview"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.18.2",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
